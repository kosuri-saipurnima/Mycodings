import java.util.*;
import java.io.*;
class Factor
{  
 static int factorial(int n){    
  if (n == 0)    
    return 1;    
  else    
    return(n * factorial(n-1));    
 }    
 public static void main(String args[])
{  
Scanner sc=new Scanner(System.in);
  int i,fact=1;  
  int n;
n=sc.nextInt();
  fact = factorial(n);   
  System.out.println("Factorial of "+n+" is: "+fact);    
 }  
}  
