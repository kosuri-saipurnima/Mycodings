import java.util.*;
import java.io.*;
class Nevenodd
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int n;
n=sc.nextInt();
int r,s=0,s1=0;
while(n>0)
{
r=n%10;
if(r%2!=0)
s=(s*10)+r;
else
s1=(s1*10)+r;
n=n/10;
}
System.out.println("odd digits:"+s+" "+"even digits:"+s1);
}
}




